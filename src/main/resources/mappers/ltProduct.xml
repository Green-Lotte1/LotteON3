<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.kr.lotteon.mapper.LtProductMapper">

	<!-- Prod_List_Test -->
	<select id="selectProducts" resultType="co.kr.lotteon.dto.LtProductDTO">
		SELECT * FROM `lt_product`
	</select>

	<insert id="insertLtProduct">

		INSERT INTO `lt_product` VALUES (#{prodNo}, #{prodCate1}, #{prodCate2}, #{prodName}, #{descript}, #{company},
										#{seller}, #{price}, #{discount}, #{point}, #{stock}, #{sold}, #{delivery},
										#{hit}, #{score}, #{review}, #{thumb1}, #{thumb2}, #{thumb3}, #{detail},
										#{status}, #{duty}, #{receipt}, #{bizType}, #{origin}, #{ip}, #{rDate},
										#{wDate}, #{isRemoved}, #{etc1}, #{etc2}, #{etc3}, #{etc4}, #{etc5}
		)
	</insert>

</mapper>
